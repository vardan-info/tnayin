// then press Enter. You can now see whitespace characters in your code.
public class Main {
    public static void main(String[] args) {
        int sum = 0;
        for(int i = 20; i < 50; i++){
            if(i%2!=0 ){
                sum +=i;
            }
        }
        System.out.println(sum);
        System.out.println(21+23+25+27+29+31+33+35+37+39+41+43+45+47+49);
    }
}

